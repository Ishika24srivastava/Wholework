package test;
import java.util.Scanner;

public class Right {
	public static void main(String[]args)
	{
		int i,j,n;
		System.out.print("Input no of rows= ");
		Scanner sc=new Scanner(System.in);
			n=sc.nextInt();
			for(i=1;i<=n;i++) {
				for(j=1;j<=i;j++)
				{
					System.out.println(j);
					System.out.println("");
					
				}
			}
		
	}

}
