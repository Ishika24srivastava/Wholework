package javaprogram;

public class Pyramid {

	public static void main(String[] args) {
		
			int i,j,r=5;
			int num=1;
		    for(i=1;i<=r;i++)
		    {
		        for(j=1;j<=i;j++)
		        {
		            System.out.print(num+" ");
		            num++;
		        }
		        
		        System.out.println();
		    }
		    }
		}

